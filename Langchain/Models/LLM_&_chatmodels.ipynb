    "load_dotenv()\n",
    "\n",
    "model = ChatOpenAI(model='gpt-4', temperature=1.5, max_completion_tokens=10)\n",
    "\n",
    "result = model.invoke(\"Write a 5 line poem on cricket\")\n",
    "\n",
    "print(result.content)"
